{
  "name": "kikoba-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:functions": "cd functions && npm run build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently --kill-others --success first \"npm run emulators\" \"ng serve --configuration test\" \"wait-on http-get://localhost:4200 && $(npm bin)/cypress run\"",
    "e2e:interactive": "concurrently \"npm run emulators\" \"ng serve --configuration test\" \"wait-on http-get://localhost:4200 && $(npm bin)/cypress open\"",
    "emulators": "firebase emulators:start --project group-saving-test",
    "test:emulate": "cross-env FIREBASE_AUTH_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.auth.port)\" FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.database.port)\" FIRESTORE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.firestore.port)\" yarn test:open",
    "deploy": "ng deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "~13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@angular/service-worker": "~13.1.2",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "bootstrap": "^5.1.3",
    "exceljs": "^4.3.0",
    "firebase": "^9.4.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "xlsx": "^0.17.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1301.3",
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@angular/language-service": "~13.1.2",
    "@ngrx/schematics": "^13.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.5.0",
    "firebase-tools": "^9.6.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "open": "^7.0.3",
    "protractor": "~7.0.0",
    "tailwindcss": "^3.0.16",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4",
    "wait-on": "^5.2.1"
  }
}
